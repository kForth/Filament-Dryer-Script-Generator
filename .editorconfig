# This file is for unifying the coding style for different editors and IDEs
# http://editorconfig.org

root = true

[*]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true
charset = utf-8
end_of_line = lf

[**.py]
indent_style = space
indent_size = 4

[**.js]
indent_style = space
indent_size = 4

[*.bat]
indent_style = space
end_of_line = crlf

[LICENSE]
insert_final_newline = false

[Makefile]
indent_style = space
